{
  "compilerOptions": {
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "noUncheckedIndexedAccess": true,
    "paths": {
      "~/*": ["./src/*"]
    },
    "strict": true,
    "strictNullChecks": true
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "src",
    "typings",
    "app.config.ts",
    "drizzle.config.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
