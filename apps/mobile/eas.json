{
  "build": {
    "app-store": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_TEST_FLIGHT": "false"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal"
    },
    "simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "test-flight": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_TEST_FLIGHT": "true"
      }
    }
  },
  "cli": {
    "appVersionSource": "local"
  },
  "submit": {
    "app-store": {},
    "test-flight": {}
  }
}
