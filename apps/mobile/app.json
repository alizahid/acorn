{
  "expo": {
    "name": "Acorn",
    "slug": "acorn",
    "scheme": "acorn",
    "version": "1.0.0",
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fff",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      }
    },
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d7d5acc-3688-4cd2-b93f-52391f665348"
      }
    },
    "icon": "./assets/images/icon.png",
    "ios": {
      "bundleIdentifier": "blue.acorn"
    },
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      "expo-secure-store",
      "expo-video"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "url": "https://u.expo.dev/edf2549b-dfeb-447c-8d58-77a5f6c52174"
    },
    "userInterfaceStyle": "automatic"
  }
}
